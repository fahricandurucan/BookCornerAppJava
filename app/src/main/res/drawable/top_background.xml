<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item>
    <shape android:shape="rectangle">
        <corners android:bottomRightRadius="30dp"
            android:bottomLeftRadius="30dp"/>
        <gradient
